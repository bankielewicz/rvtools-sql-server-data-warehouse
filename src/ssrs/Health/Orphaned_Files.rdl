<?xml version="1.0" encoding="utf-8"?>
<Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">
  <Description>Identify VMDK files not linked to registered VMs</Description>
  <Author>RVTools Data Warehouse</Author>
  <DataSources>
    <DataSource Name="RVToolsDW">
      <ConnectionProperties>
        <DataProvider>SQL</DataProvider>
        <ConnectString>Data Source=localhost;Initial Catalog=RVToolsDW</ConnectString>
        <IntegratedSecurity>true</IntegratedSecurity>
      </ConnectionProperties>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="OrphanedFiles">
      <Query>
        <DataSourceName>RVToolsDW</DataSourceName>
        <CommandText>
SELECT File_Name, Datastore, File_Type, File_Size_GiB, IsOrphaned, VI_SDK_Server
FROM [Reporting].[vw_Health_Orphaned_Files]
WHERE (@VI_SDK_Server IS NULL OR VI_SDK_Server = @VI_SDK_Server)
  AND (@ShowOrphanedOnly = 0 OR IsOrphaned = 1)
ORDER BY IsOrphaned DESC, File_Size_GiB DESC
        </CommandText>
      </Query>
      <Fields>
        <Field Name="File_Name"><DataField>File_Name</DataField></Field>
        <Field Name="Datastore"><DataField>Datastore</DataField></Field>
        <Field Name="File_Type"><DataField>File_Type</DataField></Field>
        <Field Name="File_Size_GiB"><DataField>File_Size_GiB</DataField></Field>
        <Field Name="IsOrphaned"><DataField>IsOrphaned</DataField></Field>
        <Field Name="VI_SDK_Server"><DataField>VI_SDK_Server</DataField></Field>
      </Fields>
    </DataSet>
  </DataSets>
  <ReportParameters>
    <ReportParameter Name="ShowOrphanedOnly">
      <DataType>Boolean</DataType>
      <DefaultValue><Values><Value>true</Value></Values></DefaultValue>
      <Prompt>Show Orphaned Only</Prompt>
    </ReportParameter>
    <ReportParameter Name="VI_SDK_Server">
      <DataType>String</DataType>
      <Nullable>true</Nullable>
      <AllowBlank>true</AllowBlank>
      <Prompt>vCenter Server (blank for all)</Prompt>
    </ReportParameter>
  </ReportParameters>
  <ReportSections>
    <ReportSection>
      <Body>
        <ReportItems>
          <Tablix Name="FilesTable">
            <TablixBody>
              <TablixColumns>
                <TablixColumn><Width>2in</Width></TablixColumn>
                <TablixColumn><Width>1.2in</Width></TablixColumn>
                <TablixColumn><Width>0.6in</Width></TablixColumn>
                <TablixColumn><Width>0.8in</Width></TablixColumn>
                <TablixColumn><Width>0.6in</Width></TablixColumn>
              </TablixColumns>
              <TablixRows>
                <TablixRow>
                  <Height>0.25in</Height>
                  <TablixCells>
                    <TablixCell><CellContents><Textbox Name="H1"><Paragraphs><Paragraph><TextRuns><TextRun><Value>File Name</Value><Style><FontWeight>Bold</FontWeight></Style></TextRun></TextRuns></Paragraph></Paragraphs></Textbox></CellContents></TablixCell>
                    <TablixCell><CellContents><Textbox Name="H2"><Paragraphs><Paragraph><TextRuns><TextRun><Value>Datastore</Value><Style><FontWeight>Bold</FontWeight></Style></TextRun></TextRuns></Paragraph></Paragraphs></Textbox></CellContents></TablixCell>
                    <TablixCell><CellContents><Textbox Name="H3"><Paragraphs><Paragraph><TextRuns><TextRun><Value>Type</Value><Style><FontWeight>Bold</FontWeight></Style></TextRun></TextRuns></Paragraph></Paragraphs></Textbox></CellContents></TablixCell>
                    <TablixCell><CellContents><Textbox Name="H4"><Paragraphs><Paragraph><TextRuns><TextRun><Value>Size (GiB)</Value><Style><FontWeight>Bold</FontWeight></Style></TextRun></TextRuns></Paragraph></Paragraphs></Textbox></CellContents></TablixCell>
                    <TablixCell><CellContents><Textbox Name="H5"><Paragraphs><Paragraph><TextRuns><TextRun><Value>Orphan</Value><Style><FontWeight>Bold</FontWeight></Style></TextRun></TextRuns></Paragraph></Paragraphs></Textbox></CellContents></TablixCell>
                  </TablixCells>
                </TablixRow>
                <TablixRow>
                  <Height>0.25in</Height>
                  <TablixCells>
                    <TablixCell><CellContents><Textbox Name="D1"><Paragraphs><Paragraph><TextRuns><TextRun><Value>=Fields!File_Name.Value</Value></TextRun></TextRuns></Paragraph></Paragraphs></Textbox></CellContents></TablixCell>
                    <TablixCell><CellContents><Textbox Name="D2"><Paragraphs><Paragraph><TextRuns><TextRun><Value>=Fields!Datastore.Value</Value></TextRun></TextRuns></Paragraph></Paragraphs></Textbox></CellContents></TablixCell>
                    <TablixCell><CellContents><Textbox Name="D3"><Paragraphs><Paragraph><TextRuns><TextRun><Value>=Fields!File_Type.Value</Value></TextRun></TextRuns></Paragraph></Paragraphs></Textbox></CellContents></TablixCell>
                    <TablixCell><CellContents><Textbox Name="D4"><Paragraphs><Paragraph><TextRuns><TextRun><Value>=Fields!File_Size_GiB.Value</Value><Style><Format>0.00</Format></Style></TextRun></TextRuns></Paragraph></Paragraphs></Textbox></CellContents></TablixCell>
                    <TablixCell><CellContents><Textbox Name="D5"><Paragraphs><Paragraph><TextRuns><TextRun><Value>=IIF(Fields!IsOrphaned.Value = 1, "Yes", "No")</Value></TextRun></TextRuns></Paragraph></Paragraphs></Textbox></CellContents></TablixCell>
                  </TablixCells>
                </TablixRow>
              </TablixRows>
            </TablixBody>
            <DataSetName>OrphanedFiles</DataSetName>
            <NoRowsMessage>No file data available. Run RVTools with -GetFileInfo parameter.</NoRowsMessage>
          </Tablix>
        </ReportItems>
        <Height>2in</Height>
      </Body>
      <Width>6in</Width>
      <Page>
        <PageHeader>
          <Height>0.5in</Height>
          <PrintOnFirstPage>true</PrintOnFirstPage>
          <PrintOnLastPage>true</PrintOnLastPage>
          <ReportItems>
            <Textbox Name="ReportTitle">
              <Paragraphs><Paragraph><TextRuns><TextRun><Value>Orphaned Files Report</Value><Style><FontSize>16pt</FontSize><FontWeight>Bold</FontWeight></Style></TextRun></TextRuns></Paragraph></Paragraphs>
              <Top>0.1in</Top><Left>0.1in</Left><Height>0.3in</Height><Width>4in</Width>
            </Textbox>
          </ReportItems>
        </PageHeader>
      </Page>
    </ReportSection>
  </ReportSections>
  <rd:ReportID>Orphaned_Files</rd:ReportID>
</Report>
